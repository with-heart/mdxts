{
  "name": "mdxts",
  "version": "0.5.0",
  "description": "Generate a knowledge graph of your MDX and TypeScript files.",
  "author": "Travis Arnold",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/souporserious/mdxts",
    "directory": "packages/mdxts"
  },
  "bugs": "https://github.com/souporserious/mdxts/issues",
  "homepage": "https://mdxts.dev",
  "keywords": [
    "markdown",
    "md",
    "mdx",
    "typescript",
    "ts",
    "tsx",
    "documentation",
    "docgen",
    "react"
  ],
  "files": [
    "dist/*"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "require": "./dist/cjs/components/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.js",
      "require": "./dist/cjs/components/*.js"
    },
    "./loader": {
      "types": "./dist/loader/index.d.ts",
      "import": "./dist/loader/index.js",
      "require": "./dist/cjs/loader/index.js"
    },
    "./next": {
      "types": "./dist/next/index.d.ts",
      "import": "./dist/cjs/next/index.js",
      "require": "./dist/cjs/next/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "components": [
        "./dist/components/index.d.ts"
      ],
      "components/*": [
        "./dist/components/*.d.ts"
      ],
      "loader": [
        "./dist/loader/index.d.ts"
      ],
      "next": [
        "./dist/next/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "pnpm build:esm && pnpm build:cjs && pnpm build:types",
    "build:esm": "swc ./src -d ./dist --config-file .swcrc-build",
    "build:cjs": "tsup",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "pnpm build:esm --watch",
    "test": "jest"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-stringify": "^10.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0"
  },
  "dependencies": {
    "@manypkg/find-root": "^2.2.1",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/mdx": "^2.3.0",
    "@next/mdx": "14.0.4",
    "@preact/signals-core": "^1.5.1",
    "@sindresorhus/slugify": "^2.2.1",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.100",
    "@tsxmod/utils": "^0.1.0",
    "@types/hast": "^3.0.3",
    "@types/mdast": "^4.0.3",
    "@types/mdx": "^2.0.10",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@types/title": "^3.4.3",
    "@types/webpack-env": "1.18.4",
    "@typescript/ata": "^0.9.4",
    "@typescript/vfs": "^1.5.0",
    "case-anything": "^2.1.13",
    "chokidar": "^3.5.3",
    "color": "^4.2.3",
    "copy-webpack-plugin": "^11.0.0",
    "esbuild": "^0.19.9",
    "estree-util-value-to-estree": "^3.0.1",
    "fast-glob": "^3.3.2",
    "gray-matter": "^4.0.3",
    "hast-util-to-jsx-runtime": "^2.3.0",
    "hast-util-to-string": "^3.0.0",
    "mdast-util-to-string": "^4.0.0",
    "mitt": "^3.0.1",
    "node-fetch": "^3.3.2",
    "node-polyfill-webpack-plugin": "^3.0.0",
    "prettier": "^3.1.1",
    "read-package-up": "^11.0.0",
    "rehype-infer-reading-time-meta": "^2.0.0",
    "rehype-shift-heading": "^2.0.0",
    "remark-gemoji": "^8.0.0",
    "remark-gfm": "^4.0.0",
    "remark-github": "^12.0.0",
    "remark-squeeze-paragraphs": "^6.0.0",
    "remark-strip-badges": "^7.0.0",
    "remark-typography": "^0.6.15",
    "remark-unwrap-images": "^4.0.0",
    "rollup": "^4.9.0",
    "rollup-plugin-dts": "^6.1.0",
    "server-only": "0.0.1",
    "shiki": "^0.14.6",
    "title": "^3.5.3",
    "ts-morph": "^21.0.1",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0",
    "unist-util-visit-parents": "^6.0.1",
    "vfile": "^6.0.1",
    "vscode-oniguruma": "^1.7.0",
    "vscode-textmate": "^8.0.0"
  }
}
